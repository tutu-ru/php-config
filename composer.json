{
  "name": "tutu-ru/lib-config",
  "license": "proprietary",
  "description": "Config provider",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "TutuRu\\Config\\": "src/",
      "TutuRu\\Tests\\Config\\JsonConfig\\": "tests/JsonConfig/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TutuRu\\Tests\\Config\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit -c phpunit.xml --colors=always"
    ],
    "cs": [
      "vendor/bin/phpcs --standard=.phpcs.xml ."
    ]
  }
}
